#    General axioms
#
#################################################################


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Manifest
                :Offering
                :Request
                :Confirmation
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Allocated
                :Pending
                :Preinit
                :Provisioned
                :Unallocated
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Active
                :Cleaned
                :Error
                :Initialized
                :Installed
                :NotYetInitialized
                :Ready
                :Removing
                :Started
                :Stopped
                :Stopping
                :Uncompleted
                :Updating
              )
] .


